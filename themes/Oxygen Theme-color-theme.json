{
	"name": "Oxygen Theme",
	"type": "dark",
	"tokenColors": [
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#94c08d"
			}
		},
		{
			"name": "String Escape",
			"scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
			"settings": {
				"foreground": "#A3F7BF"
			}
		},
		{
			"name": "Boolean",
			"scope": "constant.language.boolean",
			"settings": {
				"foreground": "#5F85DB"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#FFC045"
			}
		},
		{
			"name": "Identifier",
			"scope": "variable, support.variable, support.constant, meta.definition.variable entity.name.function",
			"settings": {
				"foreground": "#8ec7ff"
			}
		},
		{
			"name": "Identifier support class",
			"scope": "support.class",
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword.operator.class",
			"settings": {
				"foreground": "#636987"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, modifier, variable.language.this, support.type.object",
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"name": "Keyword this and language",
			"scope": "variable.language.this, constant.language",
			"settings": {
				"foreground": "#e66f47"
			}
		},
		{
			"name": "Keyword logical",
			"scope": "keyword.operator.logical",
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Function call",
			"scope": "entity.name.function, support.function",
			"settings": {
				"foreground": "#cab582"
			}
		},
		{
			"name": "Storage Type",
			"scope": "storage.type",
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"name": "Storage modifier",
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#D65A31"
			}
		},
		{
			"name": "Storage function",
			"scope": "storage.type.function",
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"name": "Modules",
			"scope": "support.module, support.node",
			"settings": {
				"foreground": "#8ec7ff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Type",
			"scope": "support.type",
			"settings": {
				"foreground": "#A3F7BF"
			}
		},
		{
			"name": "Type",
			"scope": "entity.name.type, entity.other.inherited-class",
			"settings": {
				"foreground": "#E93B81"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
			"foreground": "#636987",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Class",
			"scope": "entity.name.type.class",
			"settings": {
				"foreground": "#A3F7BF"
			}
		},
		{
			"name": "Class variable",
			"scope": "variable.object.property, meta.field.declaration entity.name.function",
			"settings": {
				"foreground": "#A3F7BF"
			}
		},
		{
			"name": "Class method",
			"scope": "meta.definition.method entity.name.function",
			"settings": {
				"foreground": "#A3F7BF"
			}
		},
		{
			"name": "Function definition",
			"scope": "meta.function entity.name.function",
			"settings": {
				"foreground": "#F9F7CF"
			}
		},
		{
			"name": "Template expression",
			"scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
			"settings": {
				"foreground": "#5F85DB"
			}
		},
		{
			"name": "Reset embedded/template expression colors",
			"scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "YAML key",
			"scope": "entity.name.tag.yaml",
			"settings": {
				"foreground": "#8ec7ff"
			}
		},
		{
			"name": "JSON key",
			"scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
			"settings": {
				"foreground": "#8ec7ff"
			}
		},
		{
			"name": "JSON constant",
			"scope": "constant.language.json",
			"settings": {
				"foreground": "#5F85DB"
			}
		},
		{
			"name": "CSS class",
			"scope": "entity.other.attribute-name.class",
			"settings": {
				"foreground": "#5F85DB"
			}
		},
		{
			"name": "CSS ID",
			"scope": "entity.other.attribute-name.id",
			"settings": {
				"foreground": "#ff86ab"
			}
		},
		{
			"name": "CSS tag",
			"scope": "source.css entity.name.tag",
			"settings": {
				"foreground": "#A3F7BF"
			}
		},
		{
			"name": "HTML tag outer",
			"scope": "meta.tag, punctuation.definition.tag",
			"settings": {
				"foreground": "#5F85DB"
			}
		},
		{
			"name": "HTML tag inner",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#8ec7ff"
			}
		},
		{
			"name": "HTML tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#FFC045"
			}
		},
		{
			"name": "Markdown heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#5F85DB"
			}
		},
		{
			"name": "Markdown link text",
			"scope": "text.html.markdown meta.link.inline, meta.link.reference",
			"settings": {
				"foreground": "#8ec7ff"
			}
		},
		{
			"name": "Markdown list item",
			"scope": "text.html.markdown beginning.punctuation.definition.list",
			"settings": {
				"foreground": "#5F85DB"
			}
		},
		{
			"name": "Markdown italic",
			"scope": "markup.italic",
			"settings": {
				"foreground": "#8ec7ff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown bold",
			"scope": "markup.bold",
			"settings": {
				"foreground": "#8ec7ff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown bold italic",
			"scope": "markup.bold markup.italic, markup.italic markup.bold",
			"settings": {
				"foreground": "#8ec7ff",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Markdown code block",
			"scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
			"settings": {
				"foreground": "#FF004D"
			}
		},
		{
			"name": "Markdown inline code",
			"scope": "markup.inline.raw.string.markdown",
			"settings": {
				"foreground": "#FF004D"
			}
		},
		{
			"name": "INI property name",
			"scope": "keyword.other.definition.ini",
			"settings": {
				"foreground": "#8ec7ff"
			}
		},
		{
			"name": "INI section title",
			"scope": "entity.name.section.group-title.ini",
			"settings": {
				"foreground": "#5F85DB"
			}
		},
		{
			"name": "C# class",
			"scope": "source.cs meta.class.identifier storage.type",
			"settings": {
				"foreground": "#A3F7BF",
				"fontStyle": "underline"
			}
		},
		{
			"name": "C# class method",
			"scope": "source.cs meta.method.identifier entity.name.function",
			"settings": {
				"foreground": "#A3F7BF"
			}
		},
		{
			"name": "C# function call",
			"scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
			"settings": {
				"foreground": "#FFC045"
			}
		},
		{
			"name": "C# type",
			"scope": "source.cs storage.type",
			"settings": {
				"foreground": "#A3F7BF"
			}
		},
		{
			"name": "C# return type",
			"scope": "source.cs meta.method.return-type",
			"settings": {
				"foreground": "#A3F7BF"
			}
		},
		{
			"name": "C# preprocessor",
			"scope": "source.cs meta.preprocessor",
			"settings": {
				"foreground": "#636987"
			}
		},
		{
			"name": "C# namespace",
			"scope": "source.cs entity.name.type.namespace",
			"settings": {
				"foreground": "#FFFFFF"
			}
		}
	],
	"colors": {
		"focusBorder": "#5F85DB",
		"foreground": "#FFFFFF",
		"button.background": "#5F85DB",
		"button.foreground": "#ffffff",
		"dropdown.background": "#343848",
		"input.background": "#343848",
		"inputOption.activeBorder": "#5F85DB",
		"list.activeSelectionBackground": "#5F85DB80",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.dropBackground": "#5F85DB80",
		"list.focusBackground": "#5F85DB80",
		"list.focusForeground": "#FFFFFF",
		"list.highlightForeground": "#5F85DB",
		"list.hoverBackground": "#FFFFFF1a",
		"list.inactiveSelectionBackground": "#FFFFFF33",
		"activityBar.background": "#2e313f",
		"activityBar.dropBorder": "#5F85DB80",
		"activityBarBadge.background": "#5F85DB",
		"activityBarBadge.foreground": "#ffffff",
		"badge.background": "#5F85DB",
		"badge.foreground": "#ffffff",
		"sideBar.background": "#272a36",
		"sideBarSectionHeader.background": "#2e313f",
		"editorGroup.dropBackground": "#5F85DB80",
		"editorGroup.focusedEmptyBorder": "#5F85DB",
		"editorGroupHeader.tabsBackground": "#272a36",
		"tab.border": "#00000033",
		"tab.activeBorder": "#5F85DB",
		"tab.activeBackground": "#475494",
		"tab.inactiveBackground": "#2e313f",
		"tab.activeModifiedBorder": "#5F85DB",
		"tab.inactiveModifiedBorder": "#394f83",
		"tab.unfocusedActiveModifiedBorder": "#4c6aaf",
		"tab.unfocusedInactiveModifiedBorder": "#394f83",
		"editor.background": "#21232d",
		"editor.foreground": "#FFFFFF",
		"editorLineNumber.foreground": "#FFFFFF4d",
		"editorLineNumber.activeForeground": "#5F85DB",
		"editor.lineHighlightBorder": "#FFFFFF1a",
		"editor.lineHighlightBackground": "#393E46",
		"editor.rangeHighlightBackground": "#FFFFFF0d",
		"editorWidget.background": "#272a36",
		"editorHoverWidget.background": "#272a36",
		"editorMarkerNavigation.background": "#272a36",
		"peekView.border": "#5F85DB",
		"peekViewEditor.background": "#1a1c24",
		"peekViewResult.background": "#272a36",
		"peekViewTitle.background": "#21232d",
		"panel.background": "#272a36",
		"panel.border": "#FFFFFF1a",
		"panelTitle.activeBorder": "#FFFFFF80",
		"panelTitle.inactiveForeground": "#FFFFFF80",
		"statusBar.background": "#1597BB",
		"statusBar.debuggingBackground": "#5F85DB",
		"statusBar.debuggingForeground": "#ffffff",
		"statusBar.noFolderBackground": "#29A19C",
		"statusBarItem.activeBackground": "#5F85DB80",
		"statusBarItem.hoverBackground": "#FFFFFF1a",
		"statusBarItem.remoteBackground": "#5F85DB",
		"statusBarItem.remoteForeground": "#ffffff",
		"titleBar.activeBackground": "#1a1c24",
		"pickerGroup.border": "#FFFFFF1a",
		"debugToolBar.background": "#2e313f",
		"selection.background": "#5F85DB",
		"terminal.background": "#111318",
		"terminal.foreground": "#ffffff",
	}
}
